{
  "family": "documentportaltd",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::619071324395:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "8192",
  "containerDefinitions": [
    {
      "name": "document-portal-container",
      "image": "619071324395.dkr.ecr.us-east-2.amazonaws.com/documentportalliveclass",
      "essential": true,

      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],

      "environment": [
        {
          "name": "LANGCHAIN_PROJECT",
          "value": "DOCUMENT PORTAL"
        }
      ],

      "secrets": [
        {
          "name": "GROQ_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:619071324395:secret:apikeys-l3d6aO:GROQ_API_KEY"
        },
        {
          "name": "GOOGLE_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-2:619071324395:secret:apikeys-l3d6aO:GOOGLE_API_KEY"
        }
      ],

      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/documentportaltd",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}
